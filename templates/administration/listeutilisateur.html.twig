                                <td>{{ client.email }}</td>
                                <td>{{ client.telephone }}</td>
                                <td><a id="btn-cache-{{ client.id }}" href="{{ path('compteclient', {'id': client.id}) }}">Voir</a></td>
                            </tr>
                            {% endfor %}


